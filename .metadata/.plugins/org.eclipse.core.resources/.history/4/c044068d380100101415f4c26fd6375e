package factory;

import java.math.BigDecimal;

import exceptions.StrException;
import models.Appointment;
import models.Service;

public class SalonFactory {
	public Service createService(int id, String serviceName, BigDecimal price) {
		Service service = null;
		try {
			service = new Service(id, serviceName, price);
		} catch (StrException e) {
            e.printStackTrace(); // Você ainda pode printar no console para debug, se necessário
		}
		return service;
	}
	
	public Appointment createAppointment()
}
